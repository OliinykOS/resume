<main>
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <p class="text-center text-uppercase">{{description}}</p>
    {{/with}}
    <br>
    {{#with section}}
    <div class="card">
        {{#with about_us}}
        <div class="card-body">
            {{#with title_list}}
                <div class="display-6 text-end">{{main}}</div>
                <br>
                <h2 class="text-end">{{sub}}</h2>
                <br>
            {{/with}}
            <button type="button" class="btn btn-outline-info">{{button.text}}</button>
        </div>
        {{/with}}
    </div>
    <br>
    <div class="card">
        {{#with advantage}}
        <div class="card-body">
            <div class="display-5 text-muted">{{title}}</div>
            <br>
            <div class="card">
                {{#with efficiency}}
                <div class="card-body">
                    <div class="display-4 text-muted">{{title}}</div>
                    <br>
                    <h6>{{sub_title}}</h6>
                    <br>
                    <p>{{description}}</p>
                    <br>
                    <button type="button" class="btn btn-outline-warning">{{button}}</button>
                </div>
                {{/with}}
            </div>
            <br>
            <div class="card">
                {{#with innovation}}
                <div class="card-body">
                    <div class="display-4 text-center text-muted">{{title}}</div>
                    <br>
                    <h6 class="text-center">{{sub_title}}</h4>
                    <br>
                    <p class="text-center ">{{description}}</p>
                    <br>
                    <button type="button" class="btn btn-outline-warning">{{button}}</button>
                </div>
                {{/with}}
            </div>
            <br>
            <div class="card">
                {{#with collaboration}}
                <div class="card-body">
                    <div class="display-4 text-end text-muted">{{title}}</div>
                    <br>
                    <h6 class="text-end">{{sub_title}}</h6>
                    <br>
                    <p class="text-end">{{description}}</p>
                    <br>
                    <button type="button" class="btn btn-outline-warning">{{button}}</button>
                </div>
                {{/with}}
            </div>
        </div>
        {{/with}}
    </div>
    <br>
    <div class="card">
        {{#with agreement}}
        <div class="card-body">
            <div class="display-5">{{title}}</div>
            <br>
            <div class="card">
                {{#with info}}
                <div class="card-body">
                    <h5>{{sub_title}}</h5>
                    <br>
                    <div class="card">
                        {{#with detail}}
                        <div class="card-body">
                            <p>{{text}}</p>
                            <br>
                            <button type="button" class="btn btn-success">{{button}}</button>
                        </div>
                        {{/with}}
                    </div>
                </div>
                {{/with}}            
            </div>
        </div>
        {{/with}}
    </div>
    {{/with}}
</main>